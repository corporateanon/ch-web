        "react-firebaseui": "^1.0.10",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-scripts": "^3.1.1",
        "recompose": "^0.26.0",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
